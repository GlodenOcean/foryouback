 <tlib-version>1.0</tlib-version>
 <uri>colud/ui/function</uri>
 
 <short-name>front</short-name>
 	<function>      
         <description>substring string</description><!-- 对这个EL方法的描述   -->      
         <name>subString</name><!-- 调用EL方法的名称 -->      
         <function-class>framework.ui.HCUIFunction</function-class>      
