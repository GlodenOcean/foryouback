           java.lang.String subString(java.lang.String,int,int)     
         </function-signature>      
         <example>${subString(str,int,int)}</example><!-- 例如 -->      
     </function>  
     <function>      
         <description>shortString</description><!-- 对这个EL方法的描述   -->      
         <name>shortString</name><!-- 调用EL方法的名称 -->      
         <function-class>framework.ui.HCUIFunction</function-class>      
         <function-signature>      
           java.lang.String shortString(java.lang.String,int,int)     
         </function-signature>      
         <example>${shortString(str,int,int)}</example><!-- 例如 -->      
     </function>  
</taglib>
